generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Questao {
  id String @id @default(uuid())
  ano Int
  curso String
  materia String
  categoria String
  enunciado String
  alternativaA String
  alternativaB String
  alternativaC String
  alternativaD String
  alternativaE String
  respostaCorreta String @db.Char(1)
  peso Int
}